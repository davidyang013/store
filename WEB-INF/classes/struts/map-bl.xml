<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!-- Action?????? -->
	<package name="bl" namespace="/bl" extends="common">
		<action name="BrandManagerAction_*" class="com.yu.store.bl.action.BrandManagerAction" method="{1}">
			<result name="query">/pages/bl/brand.jsp</result>
			<result name="dtl">/pages/bl/brandDtl.jsp</result>
		</action>
		
		<action name="IntoStoreManagerAction_*" class="com.yu.store.bl.action.IntoStoreManagerAction" method="{1}">
			<result name="query">/pages/bl/intoStore.jsp</result>
			<result name="dtl">/pages/bl/intoStoreDtl.jsp</result>
		</action>
		
		<action name="OutStoreManagerAction_*" class="com.yu.store.bl.action.OutStoreManagerAction" method="{1}">
			<result name="query">/pages/bl/outStore.jsp</result>
			<result name="add">/pages/bl/outStoreAdd.jsp</result>
			<result name="order">/pages/bl/outStoreOrder.jsp</result>
			<result name="orderDtl">/pages/bl/outStoreOrderDtl.jsp</result>
		</action>
		
		<action name="StorageManagerAction_*" class="com.yu.store.bl.action.StorageManagerAction" method="{1}">
			<result name="query">/pages/bl/storage.jsp</result>
			<result name="each">/pages/bl/storageEach.jsp</result>
			<result name="calc">/pages/bl/storageCalc.jsp</result>
		</action>
		
		<action name="ProfitManagerAction_*" class="com.yu.store.bl.action.ProfitManagerAction" method="{1}">
			<result name="query">/pages/bl/profit.jsp</result>
			<result name="each">/pages/bl/profitEach.jsp</result>
		</action>
		
		<action name="MemberManagerAction_*" class="com.yu.store.bl.action.MemberManagerAction" method="{1}">
			<result name="query">/pages/bl/member.jsp</result>
			<result name="dtl">/pages/bl/memberDtl.jsp</result>
		</action>
		
		<action name="RepairManagerAction_*" class="com.yu.store.bl.action.RepairManagerAction" method="{1}">
			<result name="query">/pages/bl/repair.jsp</result>
			<result name="dtl">/pages/bl/repairDtl.jsp</result>
			<result name="searchCustomerInfo">/pages/bl/repairSearchCustomer.jsp</result>
			<result name="searchBrandInfo">/pages/bl/repairSearchBrand.jsp</result>
			<result name="repairTransFee">/pages/bl/repairTransFee.jsp</result>
			<result name="actualFee">/pages/bl/repairActualTransFee.jsp</result>
		</action>
	</package>
	
</struts>    
